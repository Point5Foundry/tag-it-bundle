{
	"name": "pff/tag-it-bundle",
	"description": "Bundle to help use the Tag It library",
	"autoload": {
		"psr-0": { "Pff\\Bundle\\TagItBundle" }
	},
	"target-dir": "Pff/Bundle/TagItBundle",
	"repositories": [
		{
			"type": "package",
			"package": {
			"name": "aehlke/tag-it",
			"version": "dev-master",
			"source": {
                    
